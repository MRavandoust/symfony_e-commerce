{% extends 'base.html.twig' %}

{% block title %}Commande
{% endblock %}

{% block body %}
	<div class="container">

		<h1>Commande</h1>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ commande.id }}</td>
				</tr>
				<tr>
					<th>Montant</th>
					<td>{{ commande.montant }}</td>
				</tr>
				<tr>
					<th>Etat</th>
					<td>{{ commande.etat }}</td>
				</tr>
				<tr>
					<th>EnregistrementAt</th>
					<td>{{ commande.enregistrementAt ? commande.enregistrementAt|date('Y-m-d H:i:s') : '' }}</td>
				</tr>
			</tbody>
		</table>

		<a href="{{ path('app_admin_commande_index') }}">back to list</a>

		<a href="{{ path('app_admin_commande_edit', {'id': commande.id}) }}">edit</a>

		{{ include('admin_commande/_delete_form.html.twig') }}
	</div>
{% endblock %}
